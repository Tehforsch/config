set loadpath '~/projects/mastersThesis/gnuplotConfig/'

#set terminal postscript enhanced size 17cm,10cm
set terminal epslatex color size 15cm,15cm

# this puts tics on top of the data
set grid front
unset grid

# color definitions
set border linewidth 1.5

#e41a1c
#377eb8
#4daf4a
#984ea3
#ff7f00
#ffff33
#a65628
#f781bf
# Point styles
set style line 1 lc rgb '#377eb8' lt 1 lw 4 pt 2 ps 1.0
set style line 2 lc rgb '#e41a1c' lt 1 lw 4 pt 4 ps 1.0
set style line 3 lc rgb '#4daf4a' lt 1 lw 4 pt 6 ps 1.0
set style line 4 lc rgb '#984ea3' lt 1 lw 4 pt 8 ps 1.0
set style line 5 lc rgb '#ff7f00' lt 1 lw 4 pt 10 ps 1.0
set style line 6 lc rgb '#a65628' lt 1 lw 4 pt 12 ps 1.0
set style line 7 lc rgb '#f781bf' lt 1 lw 4 pt 14 ps 1.0
set style line 8 lc rgb '#ffff33' lt 1 lw 4 pt 16 ps 1.0

set style line 21 lc rgb '#377eb8' lt 1 lw 3 pt 2 ps 2.5
set style line 22 lc rgb '#e41a1c' lt 1 lw 3 pt 4 ps 2.5
set style line 23 lc rgb '#4daf4a' lt 1 lw 3 pt 6 ps 2.5
set style line 24 lc rgb '#984ea3' lt 1 lw 3 pt 8 ps 2.5
set style line 25 lc rgb '#ff7f00' lt 1 lw 3 pt 10 ps 2.5
set style line 26 lc rgb '#a65628' lt 1 lw 3 pt 12 ps 2.5
set style line 27 lc rgb '#f781bf' lt 1 lw 3 pt 14 ps 2.5
set style line 28 lc rgb '#ffff33' lt 1 lw 3 pt 16 ps 2.5

alias l='ls -alFh --color=auto'
alias gpu='git pull'
alias gs='git status' # This shadows ghostscript - I don't care
alias ga='git add'
alias gc='git commit'
alias gp='git push'
alias gd='git diff'
alias gdc='git diff --staged' # show diff between index and HEAD
alias t='$CONFIG/scripts/splitterminal.sh'
alias inst='sudo apt-get install'
alias dlc='$CONFIG/scripts/copyLastDownload.sh'
alias python2='python2.7'
alias python='python3'
alias v='vim $(fzf)'
alias rmf='rm -rf'
alias hibernate='~/projects/config/scripts/hibernate.sh'
alias shut='killall spotify; sudo shutdown -h 0'
alias pdfl='pdflatex --interaction=nonstopmode -shell-escape'
alias cpr='cp -rv'
alias track='python3 ~/resource/tracker/main.py'
alias todo='python3 ~/projects/config/scripts/addTodoistTask.py'
alias buildrust='trigger "clear; cargo rustc -- -Awarnings" $(fd ".*.rs")'
alias runrust='trigger "clear; cargo run"'
alias calc='python3 -i $HOME/resource/calculator/calculator.py'
alias pomodoro='timer 1500'

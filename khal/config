[calendars]

[[contacts_local]]
path = ~/.local/share/dav/contacts/*
type = discover

[[personal_calendar_local]]
path = ~/.local/share/dav/calendar/personal/*
type = discover

[[work_calendar_local]]
path = ~/.local/share/dav/calendar/work/*
type = discover

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

[default]
default_calendar = personal

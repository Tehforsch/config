set terminal postscript enhanced

set size 0.5, 0.5

# color definitions
set border linewidth 1.5
set style line 1 lc rgb '#0060ad' lt 1 lw 2 pt 7 # --- blue
set style line 2 lc rgb '#bf1919' lt 1 lw 2 pt 7 # --- red
set style line 3 lc rgb '#009900' lt 1 lw 2 pt 7 # --- green
set style line 4 lc rgb '#BDB76B' lt 1 lw 2 pt 7 # --- yellow/brown

set tics scale 0.75
